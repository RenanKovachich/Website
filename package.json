{
  "name": "linkspace",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.1.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^7.27.3",
    "@mui/x-date-pickers": "^6.18.1",
    "@prisma/client": "^5.8.1",
    "@tanstack/react-query": "^5.67.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "axios": "^1.6.7",
    "date-fns": "^2.29.3",
    "formik": "^2.4.5",
    "framer-motion": "^12.16.0",
    "jwt-decode": "^4.0.0",
    "notistack": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.1",
    "redis": "^5.8.3",
    "typescript": "^4.9.5",
    "uuid": "^13.0.0",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "build": "set GENERATE_SOURCEMAP=false && react-scripts build",
    "test": "vitest run --reporter=verbose",
    "test:ui": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:cov": "vitest run --coverage",
    "test:security": "npm run test && npm run test:e2e && npm run test:cov",
    "lint": "eslint .",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output json --quiet --chrome-flags='--headless' --output-path docs/test-reports/lighthouse/report",
    "test:all": "npm run test && npm run cypress:run",
    "seed": "ts-node scripts/seed.ts",
    "migrate": "ts-node scripts/migrate-to-multitenant.ts",
    "seed:multi-tenant": "npm run migrate && npm run seed",
    "analyze": "npm run build && npx serve -s build",
    "posttest": "node scripts/collect-security-evidence.js || true",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^18.19.130",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^24.0.0",
    "lighthouse": "^11.7.1",
    "ts-node": "^10.9.2",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  }
}
